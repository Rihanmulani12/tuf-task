{
    "version": 2,
    "env": {
      "DB_HOST": "@your-db-host",
      "DB_USER": "@your-db-user",
      "DB_PASSWORD": "@your-db-password",
      "DB_NAME": "@your-db-name"
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/submit",
        "dest": "index.js"
      },
      {
        "src": "/snippets",
        "dest": "index.js"
      }
    ]
  }
  